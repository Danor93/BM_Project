<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane prefHeight="358.0" prefWidth="444.0">
         <children>
            <Label layoutX="164.0" layoutY="54.0" prefHeight="65.0" prefWidth="109.0" text="Upload PDF">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label layoutX="31.0" layoutY="146.0" prefHeight="19.0" prefWidth="109.0" text="Choose a Quertar:">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <Button fx:id="BackBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#Back" text="Back">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="UploadBtn" layoutX="157.0" layoutY="248.0" mnemonicParsing="false" onAction="#UploadPDF" prefHeight="42.0" prefWidth="130.0" text="Upload File">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
            </Button>
            <Label layoutX="31.0" layoutY="191.0" prefHeight="20.0" prefWidth="109.0" text="Enter a Year:">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <ComboBox fx:id="QuertarComboBox" layoutX="157.0" layoutY="143.0" onAction="#ChooseQuertar" prefHeight="25.0" prefWidth="186.0" />
            <ComboBox fx:id="YearComboBox" layoutX="157.0" layoutY="188.0" onAction="#chooseYear" prefHeight="25.0" prefWidth="186.0" />
         </children>
      </Pane>
   </children>
</AnchorPane>
